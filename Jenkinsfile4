pipeline {
    agent any
    stages{
      stage('compilation') {
  steps {
    // One or more steps need to be included within the steps block.
    sh 'mvn compile'

  }
}  
   stage('packaging') {
  steps {
    // One or more steps need to be included within the steps block.
    sh 'mvn package'
  }
}
stage('SonarQube analysis') {
steps {
    withSonarQubeEnv(credentialsId: '074d59409968ec7d720b00bd8fc6f2636b067910', installationName: 'SonarQube-key') 
    // You can override the credential to be used
      sh 'mvn sonar:sonar'
    }
  }


}
        
        
        
        
        
    }
    
  
